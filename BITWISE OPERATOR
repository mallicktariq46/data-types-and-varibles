#include<bits/stdc++.h>
using namespace std;
int main(){
    int a,b;
    cin>>a>>b;
int z=a|b;
    cout<<z<<endl;
z=a&b;
    cout<<z<<endl;
z=a^b;
    cout<<z<<endl;
    return 0;
}
