#include<bits/stdc++.h>
using namespace std;
int main(){
    int a,b;
    cin>>a>>b;
    cout<<"ADDITION OF A NAD B:"<<" "<<a+b<<endl;
    cout<<"SUBSTRACTION OF A NAD B:"<<" "<<a-b<<endl;
    cout<<"MULTIPLICATION OF A NAD B:"<<" "<<a*b<<endl;
    float x=a,y=b;
    cout<<"DIVISION OF A AND B : "<<x/y<<endl;
    return 0;
}
/*/ "ADDITION OF A NAD B: A+B"
 SUBSTRACTION OF A NAD B: A-B
 MULTIPLICATION OF A NAD B: A*B
 DIVISION OF A AND B : x/y;
 /*/
