#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cout<<"enter an integer :";
    cin>>n;
    cout<<n<<endl;
    float b=n;
    cout<<"enter a decimal type :";
    cin>>b;
    char h;
    cout<<"ENTER A CHAR : ";
    cin>>h;
    cout<<h<<endl;
    string s1;
    cout<<"ENTER YOUR NAME : ";
    cin>>s1;
    cout<<s1<<endl;
    bool b1;
    cout<<"ENTER TRUE OR FALSE : ";
    cin>>b1;
    cout<<b1<<endl;
    return 0;
}
/*/
enter an integer : n;
float : b;
ENTER A CHAR : h
ENTER YOUR NAME : s1
ENTER TRUE OR FALSE : b1
/*/
